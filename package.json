{
  "name": "melon-json-schema-form",
  "version": "2.0.0-alpha.3",
  "description": "melon json schema form",
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=dev node tools/server.js",
    "build": "rimraf lib && NODE_ENV=production gulp build",
    "lint": "fecs src",
    "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
    "test-ci": "NODE_ENV=test karma start ./test/karma.travis.conf.js",
    "coveralls": "cat test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "docs": "esdoc -c esdoc.json",
    "setup": "rimraf asset && NODE_ENV=dev webpack --config=tools/webpack.dll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-melon/melon-json-schema-form.git"
  },
  "author": {
    "name": "ludafa",
    "email": "ludafa@outlook.com"
  },
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "test",
    "src": "src"
  },
  "dependencies": {
    "ajv": "^4.10.0",
    "ajv-i18n": "^1.3.0",
    "classnames": "^2.2.5",
    "melon": "^0.4.3",
    "melon-calendar": "^1.0.5",
    "melon-colorpicker": "^1.0.9",
    "melon-form": "^1.0.0-alpha.2",
    "melon-timepicker": "^1.0.2",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-addons-update": "^0.14.8||^15.4.0",
    "react-dom": "^15.4.1",
    "redux": "^3.6.0",
    "shallow-equal": "^1.0.0"
  },
  "devDependencies": {
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-external-helpers-insert-require": "^1.0.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "codemirror": "^5.19.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.24.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fecs": "^0.8.6",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-babel-external-helpers": "^2.0.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "nib": "^1.1.2",
    "react-addons-test-utils": "^15.4.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  },
  "babel": {
    "env": {
      "dev": {
        "presets": [
          "es2015",
          "stage-2",
          "react"
        ]
      },
      "test": {
        "presets": [
          "es2015",
          "stage-2",
          "react"
        ],
        "plugins": [
          "istanbul"
        ]
      },
      "production": {
        "presets": [
          [
            "es2015",
            {
              "loose": true,
              "modules": "umd"
            }
          ],
          "stage-2",
          "react"
        ],
        "plugins": [
          "transform-es3-property-literals",
          "transform-es3-member-expression-literals"
        ]
      }
    }
  }
}
